<%= react_component("rules/templates", { templates: @templates.as_json }) %>

<script>
  var button = Button.create(app, { label: 'Cancel' });

  button.subscribe('click', () => {
    Turbolinks.visit('<%= new_rule_path %>');
  });

  TitleBar.create(app, {
    title: 'Pick a template',
    buttons: {
      secondary: [button]
    }
  });
</script>
